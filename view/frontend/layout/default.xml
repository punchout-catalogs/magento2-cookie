<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="js_cookies">
            <action method="setTemplate">
                <argument name="template" xsi:type="string">Punchout_Cookie::js/cookie.phtml</argument>
            </action>
        </referenceBlock>
        <referenceBlock name="cookie_config">
            <action method="setTemplate">
                <argument name="template" xsi:type="string">Punchout_Cookie::html/cookie.phtml</argument>
            </action>
        </referenceBlock>
    </body>
</page>
